def prime(n):
    if (n <=1):
        return False
    for i in range(2,n ):
        if (n % i == 0):
            return False
    return True

def sod(n):
    p=sum(int(i) for i in str(n))
    return p
def digits_prime(n):
    if(n % 10 == 0):
        return False
    return True

for i in range(100, 1000):
    if (prime(i) and prime(sod(i)) and digits_prime(i)):
        print(i)
