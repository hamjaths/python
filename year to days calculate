n = int(input("enter the year:"))
a = n - 2000
b = a //4
c = a-b
d = b * 2
e = c + d
f = e % 7
print(f)
if(f==0):
    print("sunday")
if(f==1):
    print("monday")
if(f==2):
    print("tuesday")
if(f==3):
    print("wednesday")
if(f==4):
    print("thursday")
if(f==5):
    print("friday")
if(f==6):
    print("saturday")
    
